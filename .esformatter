{
  "indent": {
    "value": "  "
  },
  "plugins": [
    "esformatter-quotes",
    "esformatter-literal-notation",
    "esformatter-parseint",
    "esformatter-var-each"
  ],
  "quotes": {
    "type": "single",
    "avoidEscape": true
  },
  "whiteSpace": {
    "after": {
      "FunctionName": 1,
      "FunctionReservedWord": 1
    },
    "removeTrailing": 1
  }
}
